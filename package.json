{
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "lerna exec --no-bail -- tsc",
    "clean": "lerna exec -- shx rm -rf bench test *.d.ts *.js *.js.map",
    "docs": "node bin/docs.js",
    "link": "lerna exec -- npm link",
    "link-all": "npm run link && npm run bootstrap",
    "publish": "npm run clean && npm run build && npm run test && lerna publish --exact",
    "test": "lerna exec --concurrency=1 -- npm run test",
    "watch": "node bin/watch.js"
  },
  "author": "Mikola Lysenko",
  "devDependencies": {
    "@types/node": "^8.10.21",
    "@types/tape": "^4.2.32",
    "browserify": "^16.2.2",
    "budo": "^11.3.2",
    "envify": "^4.1.0",
    "fast-deep-equal": "^2.0.1",
    "get-port": "^4.0.0",
    "ip": "^1.1.5",
    "lerna": "^2.11.0",
    "shx": "^0.3.2",
    "tape": "^4.9.1",
    "tape-run": "^4.0.0",
    "ts-node": "^7.0.0",
    "tsify": "^4.0.0",
    "tslint": "^5.11.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "^2.9.2",
    "webworkify": "^1.5.0"
  }
}
