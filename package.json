{
  "name": "mudb",
  "version": "0.2.8",
  "description": "Real-time database for multiplayer games",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "tsc",
    "clean": "shx rm -rf clock rpc schema socket state stream *.d.ts *.js *.js.map",
    "docs": "node bin/docs.js",
    "link": "npm link && npm run clean && npm run build",
    "test": "lerna exec --concurrency=1 -- npm run test",
    "watch": "tsc -w"
  },
  "main": "core/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikolalysenko/mudb.git"
  },
  "keywords": [
    "mudb",
    "real-time",
    "multiplayer",
    "game",
    "network",
    "server",
    "client",
    "clock",
    "replicate",
    "schema",
    "binary",
    "buffer",
    "stream",
    "websocket",
    "tcp",
    "udp"
  ],
  "author": "Mikola Lysenko",
  "license": "MIT",
  "dependencies": {
    "hash.js": "^1.1.5",
    "json-stable-stringify": "^1.0.1",
    "uws": "^10.148.0"
  },
  "devDependencies": {
    "@types/node": "^8.10.21",
    "@types/tape": "^4.2.32",
    "browserify": "^16.2.2",
    "budo": "^11.3.2",
    "envify": "^4.1.0",
    "fast-deep-equal": "^2.0.1",
    "get-port": "^4.0.0",
    "ip": "^1.1.5",
    "lerna": "^2.11.0",
    "shx": "^0.3.2",
    "tape": "^4.9.1",
    "tape-run": "^4.0.0",
    "ts-node": "^7.0.0",
    "tsify": "^4.0.0",
    "tslint": "^5.11.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "^2.9.2",
    "webworkify": "^1.5.0"
  }
}
